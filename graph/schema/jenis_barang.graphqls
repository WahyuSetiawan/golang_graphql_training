type JenisBarang {
  id: Int!
  jenis_barang: String!
}

type resultJenisBarang implements resultInsert {
  status: String!
  code: Int!
  data: JenisBarang!
}

type resultGetAllJenisBarang implements resultInsert {
  status: String!
  code: Int!
  data: [JenisBarang]!
}

extend type Query {
  insertJenisBarang(jenis_barang: String!): resultJenisBarang!
  getAllJenisBarang: resultGetAllJenisBarang!
}
